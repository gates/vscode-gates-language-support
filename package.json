{
  "name": "gates-language-support",
  "displayName": "Gates Language Support",
  "description": "Gates language support.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gates",
        "aliases": [
          "Gates",
          "gates"
        ],
        "extensions": [
          ".gates"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gates",
        "scopeName": "source.gates",
        "path": "./syntaxes/gates.tmLanguage.json"
      }
    ]
  }
}
