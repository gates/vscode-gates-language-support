{
  "name": "gates",
  "displayName": "Gates",
  "description": "Gates language support for Visual Studio Code.",
  "publisher": "lujjjh",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gates/vscode-gates-language-support.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "gates",
        "aliases": [
          "Gates",
          "gates"
        ],
        "extensions": [
          ".gates"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gates",
        "scopeName": "source.gates",
        "path": "./syntaxes/gates.tmLanguage.json"
      }
    ]
  }
}
